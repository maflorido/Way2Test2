@using GitApi.Web.ViewModels.FavoritosModels;

@model FavoritosListarViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Favoritos</h2>

<table class="table table-striped table-bordered table-hover table-condensed">
    <tr>
        <th>Nome do repositório</th>
        <th>Id do repositório no git</th>
    </tr>
    @foreach (var item in Model.Itens)
    {
        <tr>
            <td>@item.NomeRepositorio</td>
            <td>@item.IdRepositorioGit</td>
        </tr>
    }    
</table>

